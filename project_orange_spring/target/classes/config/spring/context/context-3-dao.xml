<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<!-- Root Context: defines shared resources visible to all other web components -->

	<bean id="music_dao" class="dao.MusicDAO">
		<property name="sqlSession" ref="sqlSessionBean"></property>
	</bean>
	
	<bean id="applyfriend_dao" class="dao.ApplyfriendDAO">
		<property name="sqlSession" ref="sqlSessionBean"></property>
	</bean>
	<bean id="comments_dao" class="dao.CommentsDAO">
		
		<property name="sqlSession" ref="sqlSessionBean"></property>
	</bean>
	<bean id="member_dao" class="dao.MemberDAO">
		<property name="sqlSession" ref="sqlSessionBean"></property>
	</bean>
	<bean id="message_dao" class="dao.MessageDAO">
		<property name="sqlSession" ref="sqlSessionBean"></property>
	</bean>
	<bean id="myfriend_dao" class="dao.MyfriendDAO">
		<property name="sqlSession" ref="sqlSessionBean"></property>
	</bean>
	<bean id="playlist_dao" class="dao.PlaylistDAO">
		<property name="sqlSession" ref="sqlSessionBean"></property>
	</bean>
	
</beans>
